    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/integrations": "^7.114.0",
    "@sentry/nestjs": "^10.27.0",
    "@sentry/node": "^10.27.0",
    "@sentry/profiling-node": "^10.27.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "joi": "^18.0.1",
    "ms": "^2.1.3",
