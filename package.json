{
  "name": "club",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:cov": "jest --coverage --config jest.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.ts",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:reset": "cross-env NODE_ENV=test DATABASE_URL=postgresql://root:root@localhost:5432/test_club npx prisma migrate reset --force",
    "test:unit": "cross-env NODE_ENV=test DATABASE_URL=postgresql://root:root@localhost:5432/test_club npm run test",
    "test:e2e:test": "cross-env NODE_ENV=test DATABASE_URL=postgresql://root:root@localhost:5432/test_club npm run test:e2e",
    "test:all": "npm run test:reset && npm run test:unit && npm run test:e2e:test",
    "seed": "ts-node --project tsconfig.seed.json src/prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
